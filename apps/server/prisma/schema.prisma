generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int      @unique @default(autoincrement()) @db.SmallInt
  username   String   @id @db.VarChar
  password   String?  @db.VarChar
  first_name String?
  last_name  String?
  created_at DateTime @default(now()) @db.Timestamptz(6)
  role       String?
  division   String?
}